    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'epidemic_lockdown'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=epidemic_lockdown",
                    "--package=epidemic_lockdown"
                ],
                "filter": {
                    "name": "epidemic_lockdown",
                    "kind": "bin"
                }
            },
            "args": [
                "256N_graph0.dat",
                "4",
                "0.8",
                "0.2",
                "0.1",
                "0.05"
            ],
            "cwd": "${workspaceFolder}"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug unit tests in executable 'epidemic_lockdown'",
            "cargo": {
                "args": [
                    "test",
                    "--no-run",
                    "--bin=epidemic_lockdown",
                    "--package=epidemic_lockdown"
                ],
                "filter": {
                    "name": "epidemic_lockdown",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        }
    ]
}